statsGrid.SortOrder = Enum.SortOrder.LayoutOrder
statsGrid.Parent = statsContainer

local function createStatCard(name, title, initialValue, order)
	local card = createRoundedFrame(statsContainer, name, UDim2.new(0, 0, 0, 0), UDim2.new(0, 0, 0, 0), Color3.fromRGB(20, 20, 25), 6)
	card.LayoutOrder = order
	
	local cardPadding = Instance.new("UIPadding")
	cardPadding.PaddingTop = UDim.new(0, 5)
	cardPadding.PaddingLeft = UDim.new(0, 7)
	cardPadding.PaddingRight = UDim.new(0, 7)
	cardPadding.Parent = card
	
	local titleLbl = Instance.new("TextLabel")
	titleLbl.Size = UDim2.new(1, 0, 0, 11)
	titleLbl.BackgroundTransparency = 1
	titleLbl.Text = title
	titleLbl.TextColor3 = Color3.fromRGB(160, 160, 170)
	titleLbl.TextSize = 8
	titleLbl.Font = Enum.Font.Gotham
	titleLbl.TextXAlignment = Enum.TextXAlignment.Left
	titleLbl.TextTruncate = Enum.TextTruncate.AtEnd
	titleLbl.Parent = card
	
	local valueLbl = Instance.new("TextLabel")
	valueLbl.Name = "ValueLabel"
	valueLbl.Size = UDim2.new(1, 0, 0, 18)
	valueLbl.Position = UDim2.new(0, 0, 0, 16)
	valueLbl.BackgroundTransparency = 1
	valueLbl.Text = initialValue
	valueLbl.TextColor3 = Color3.fromRGB(255, 255, 255)
	valueLbl.TextSize = 12
	valueLbl.Font = Enum.Font.GothamBold
	valueLbl.TextXAlignment = Enum.TextXAlignment.Left
	valueLbl.TextTruncate = Enum.TextTruncate.AtEnd
	valueLbl.Parent = card
	
	return card, valueLbl
end

-- Create stat cards
local playersCard, playersValue = createStatCard("Players", "Players", "...", 1)
local maxPlayersCard, maxPlayersValue = createStatCard("MaxPlayers", "Max Players", "...", 2)
local latencyCard, latencyValue = createStatCard("Latency", "Ping", "...", 3)
local fpsCard, fpsValue = createStatCard("FPS", "FPS", "...", 4)
local timeCard, timeValue = createStatCard("InServer", "In Server", "...", 5)
local friendsPlayingCard, friendsPlayingValue = createStatCard("FriendsPlaying", "Friends Here", "...", 6)
local friendsTotalCard, friendsTotalValue = createStatCard("FriendsTotal", "Total Friends", "...", 7)
local friendsOnlineCard, friendsOnlineValue = createStatCard("FriendsOnline", "Friends Online", "...", 8)
local joinScriptCard, joinScriptValue = createStatCard("JoinScript", "Join Script", "Tap to copy", 9)

-- Make Join Script clickable
local joinScriptButton = Instance.new("TextButton")
joinScriptButton.Size = UDim2.new(1, 0, 1, 0)
joinScriptButton.BackgroundTransparency = 1
joinScriptButton.Text = ""
joinScriptButton.Parent = joinScriptCard

joinScriptButton.MouseButton1Click:Connect(function()
	local jobId = game.JobId
	local placeId = game.PlaceId
	local script = string.format('game:GetService("TeleportService"):TeleportToPlaceInstance(%d, "%s", game.Players.LocalPlayer)', placeId, jobId)
	setclipboard(script)
	joinScriptValue.Text = "Copied!"
	wait(2)
	joinScriptValue.Text = "Tap to copy"
end)

-- Update Functions
local joinTime = tick()

local function updateStats()
	-- Players
	local playerCount = #Players:GetPlayers()
	playersValue.Text = playerCount .. " playing"
	
	-- Max Players
	maxPlayersValue.Text = Players.MaxPlayers .. " max"
	
	-- Latency (Ping)
	local ping = Stats.PerformanceStats.Ping:GetValue()
	latencyValue.Text = math.floor(ping) .. "ms"
	
	-- FPS
	local fps = math.floor(1 / RunService.RenderStepped:Wait())
	fpsValue.Text = fps .. " fps"
	
	-- Time in server
	local timeInServer = math.floor(tick() - joinTime)
	local minutes = math.floor(timeInServer / 60)
	local seconds = timeInServer % 60
	if minutes > 0 then
		timeValue.Text = minutes .. "m " .. seconds .. "s"
	else
		timeValue.Text = seconds .. " seconds"
	end
	
	-- Friends
	local friendsInServer = 0
	local friendsList = player:GetFriendsOnline(200)
	
	for _, friend in pairs(friendsList) do
		if friend.PlaceId == game.PlaceId then
			friendsInServer = friendsInServer + 1
		end
	end
	
	friendsPlayingValue.Text = friendsInServer .. " here"
	friendsTotalValue.Text = #player:GetFriendsOnline(200) .. " friends"
	
	-- Count online friends
	local onlineCount = 0
	for _, friend in pairs(friendsList) do
		if friend.IsOnline then
			onlineCount = onlineCount + 1
		end
	end
	friendsOnlineValue.Text = onlineCount .. " online"
end

-- Update stats every second
RunService.Heartbeat:Connect(function()
	updateStats()
end)

-- Initial update
updateStats()

