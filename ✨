-- Roblox Mobile-Friendly UI with Particle Animation
-- Place this in StarterGui as a LocalScript

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Stats = game:GetService("Stats")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "BlingUI"
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = playerGui

-- Helper function to create rounded frames
local function createRoundedFrame(parent, name, size, position, bgColor, cornerRadius)
	local frame = Instance.new("Frame")
	frame.Name = name
	frame.Size = size
	frame.Position = position
	frame.BackgroundColor3 = bgColor
	frame.BorderSizePixel = 0
	frame.ClipsDescendants = true
	frame.Parent = parent
	
	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, cornerRadius or 12)
	corner.Parent = frame
	
	return frame
end

-- Add gradient to frame
local function addGradient(frame, color1, color2, rotation)
	local gradient = Instance.new("UIGradient")
	gradient.Color = ColorSequence.new(color1, color2)
	gradient.Rotation = rotation or 45
	gradient.Parent = frame
	return gradient
end

-- Toggle Button (Always visible, draggable)
local toggleButton = createRoundedFrame(screenGui, "ToggleButton", UDim2.new(0, 50, 0, 50), UDim2.new(1, -60, 0, 10), Color3.fromRGB(30, 30, 40), 12)
addGradient(toggleButton, Color3.fromRGB(40, 40, 50), Color3.fromRGB(25, 25, 35), 135)

local toggleLabel = Instance.new("TextLabel")
toggleLabel.Size = UDim2.new(1, 0, 1, 0)
toggleLabel.BackgroundTransparency = 1
toggleLabel.Text = "✨"
toggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleLabel.TextSize = 28
toggleLabel.Font = Enum.Font.GothamBold
toggleLabel.Parent = toggleButton

-- Toggle Button Drag Logic (separate from click)
local isDraggingToggle = false
local toggleDragStart = nil
local toggleStartPos = nil

toggleButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		toggleDragStart = input.Position
		toggleStartPos = toggleButton.Position
	end
end)

toggleButton.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		if toggleDragStart then
			isDraggingToggle = true
			local delta = input.Position - toggleDragStart
			toggleButton.Position = UDim2.new(
				toggleStartPos.X.Scale,
				toggleStartPos.X.Offset + delta.X,
				toggleStartPos.Y.Scale,
				toggleStartPos.Y.Offset + delta.Y
			)
		end
	end
end)

toggleButton.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		-- Only toggle if we weren't dragging
		if not isDraggingToggle then
			-- Toggle visibility with particle animation
			if not mainContainer.Visible then
				playParticleAnimation()
			else
				mainContainer.Visible = false
			end
		end
		
		-- Reset drag state
		isDraggingToggle = false
		toggleDragStart = nil
	end
end)

-- Main Container (Rectangle shape - minimal corner radius)
local mainContainer = Instance.new("Frame")
mainContainer.Name = "MainContainer"
mainContainer.Size = UDim2.new(0, 340, 0, 500)
mainContainer.Position = UDim2.new(0.5, -170, 0.5, -250)
mainContainer.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
mainContainer.BorderSizePixel = 0
mainContainer.ClipsDescendants = false
mainContainer.Visible = false
mainContainer.Parent = screenGui

local containerCorner = Instance.new("UICorner")
containerCorner.CornerRadius = UDim.new(0, 4) -- Minimal rounding for rectangle
containerCorner.Parent = mainContainer

-- Particle Formation Animation
local particleContainer = Instance.new("Frame")
particleContainer.Name = "ParticleContainer"
particleContainer.Size = UDim2.new(1, 0, 1, 0)
particleContainer.BackgroundTransparency = 1
particleContainer.ClipsDescendants = false
particleContainer.Parent = screenGui

function playParticleAnimation()
	-- Clear old particles
	for _, child in pairs(particleContainer:GetChildren()) do
		child:Destroy()
	end
	
	-- Create particles
	local numParticles = 30
	local particles = {}
	
	for i = 1, numParticles do
		local particle = Instance.new("Frame")
		particle.Size = UDim2.new(0, math.random(8, 20), 0, math.random(8, 20))
		particle.BackgroundColor3 = Color3.fromHSV(math.random(), 0.7, 1)
		particle.BorderSizePixel = 0
		particle.Position = UDim2.new(math.random(), 0, math.random(), 0)
		particle.ZIndex = 10
		particle.Parent = particleContainer
		
		local corner = Instance.new("UICorner")
		corner.CornerRadius = UDim.new(0, math.random(2, 6))
		corner.Parent = particle
		
		table.insert(particles, particle)
	end
	
	-- Show container immediately but transparent
	mainContainer.Visible = true
	mainContainer.BackgroundTransparency = 1
	for _, child in pairs(mainContainer:GetDescendants()) do
		if child:IsA("GuiObject") then
			child.BackgroundTransparency = 1
			if child:IsA("TextLabel") or child:IsA("TextButton") then
				child.TextTransparency = 1
			end
			if child:IsA("ImageLabel") then
				child.ImageTransparency = 1
			end
			if child:IsA("ScrollingFrame") then
				child.ScrollBarImageTransparency = 1
			end
		end
	end
	
	-- Animate particles to center
	local targetPos = mainContainer.AbsolutePosition
	local targetSize = mainContainer.AbsoluteSize
	
	for i, particle in pairs(particles) do
		local delay = (i - 1) * 0.02
		
		-- Calculate random position within the container
		local endX = targetPos.X + math.random(0, targetSize.X)
		local endY = targetPos.Y + math.random(0, targetSize.Y)
		
		task.wait(delay)
		
		local tween = TweenService:Create(particle, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.InOut), {
			Position = UDim2.new(0, endX, 0, endY),
			Size = UDim2.new(0, 0, 0, 0),
			BackgroundTransparency = 1
		})
		tween:Play()
	end
	
	-- Fade in main container
	task.wait(0.3)
	
	local containerTween = TweenService:Create(mainContainer, TweenInfo.new(0.3), {
		BackgroundTransparency = 0
	})
	containerTween:Play()
	
	for _, child in pairs(mainContainer:GetDescendants()) do
		if child:IsA("GuiObject") then
			local originalTrans = 0
			if child.Name == "DragHandle" then
				originalTrans = 0
			elseif child.BackgroundColor3 == Color3.fromRGB(15, 15, 20) then
				originalTrans = 1
			end
			
			TweenService:Create(child, TweenInfo.new(0.3), {
				BackgroundTransparency = originalTrans
			}):Play()
			
			if child:IsA("TextLabel") or child:IsA("TextButton") then
				TweenService:Create(child, TweenInfo.new(0.3), {
					TextTransparency = 0
				}):Play()
			end
			if child:IsA("ImageLabel") then
				TweenService:Create(child, TweenInfo.new(0.3), {
					ImageTransparency = 0
				}):Play()
			end
			if child:IsA("ScrollingFrame") then
				TweenService:Create(child, TweenInfo.new(0.3), {
					ScrollBarImageTransparency = 0
				}):Play()
			end
		end
	end
	
	-- Clean up particles
	task.wait(0.8)
	for _, particle in pairs(particles) do
		particle:Destroy()
	end
end

-- Drag Handle
local dragHandle = Instance.new("Frame")
dragHandle.Name = "DragHandle"
dragHandle.Size = UDim2.new(1, 0, 0, 40)
dragHandle.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
dragHandle.BorderSizePixel = 0
dragHandle.Parent = mainContainer

local handleCorner = Instance.new("UICorner")
handleCorner.CornerRadius = UDim.new(0, 4)
handleCorner.Parent = dragHandle

local dragIndicator = Instance.new("Frame")
dragIndicator.Size = UDim2.new(0, 40, 0, 4)
dragIndicator.Position = UDim2.new(0.5, -20, 0.5, -2)
dragIndicator.BackgroundColor3 = Color3.fromRGB(80, 80, 90)
dragIndicator.BorderSizePixel = 0
dragIndicator.Parent = dragHandle

local indicatorCorner = Instance.new("UICorner")
indicatorCorner.CornerRadius = UDim.new(1, 0)
indicatorCorner.Parent = dragIndicator

-- Make container draggable
local dragging = false
local dragInput, dragStart, startPos

dragHandle.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = mainContainer.Position
		
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

dragHandle.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		mainContainer.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)

